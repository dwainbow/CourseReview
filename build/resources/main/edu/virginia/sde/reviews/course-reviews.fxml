<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: FFD099;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.virginia.sde.reviews.CourseReviewsController">
   <children>
      <TableView fx:id="table" layoutX="-1.0" layoutY="93.0" prefHeight="308.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="timeColumn" prefWidth="75.0" text="Time" />
          <TableColumn fx:id="ratingColumn" prefWidth="150.0" text="Rating" />
            <TableColumn fx:id="commentsColumn" minWidth="0.0" prefWidth="374.0" text="Comments" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="545.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: FF9209;" text="Back" />
      <Button fx:id="addReview" layoutX="507.0" layoutY="54.0" mnemonicParsing="false" onAction="#addReview" style="-fx-background-color: FF9209;" text="Add Review" />
      <Text fx:id="courseTitle" fill="#2b3499" layoutX="14.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Course">
         <font>
            <Font name="Georgia" size="14.0" />
         </font>
      </Text>
      <Text fx:id="avgRating" fill="#2b3499" layoutX="14.0" layoutY="71.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Avg Rating">
         <font>
            <Font name="Georgia" size="14.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
